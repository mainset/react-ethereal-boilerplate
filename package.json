{
  "name": "react-ethereal-boilerplate",
  "version": "0.0.3",
  "description": "React + Redux + redux-saga lightweight starter kit",
  "scripts": {
    "start": "npm run watch:csr",
    "start:ssr": "node ./private/server.js",
    "start:serve-static": "node ./config/serve-static.js",
    "build": "npm run build:csr",
    "build:csr": "cross-env NODE_ENV=production webpack --mode production --config ./config/webpack/csr.config.js",
    "build:ssr": "cross-env NODE_ENV=production webpack --mode production --config ./config/webpack/ssr.config.js",
    "build:server": "cross-env NODE_ENV=production webpack --mode production --config ./config/webpack/server.config.js",
    "watch:csr": "cross-env NODE_ENV=development webpack-dev-server --mode development --config ./config/webpack/csr.config.js --open --inline --hot",
    "watch:ssr": "cross-env NODE_ENV=development webpack --mode development --watch --config ./config/webpack/ssr.config.js",
    "watch:server": "cross-env NODE_ENV=development webpack --mode development --watch --config ./config/webpack/server.config.js",
    "watch:serve-static": "nodemon ./private/server.js",
    "lint:js": "eslint --ext .js src/"
  },
  "main": "index.js",
  "repository": "git@github.com:mainset/react-ethereal-boilerplate.git",
  "author": "Yevhen Uzhva <yevhen.uzhva@gmail.com>",
  "license": "MIT",
  "private": false,
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=5.0.0",
    "yarn": "^1.7.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "cross-env": "^5.2.0",
    "eslint": "^5.7.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.1.2",
    "nodemon": "^1.18.9",
    "redux-devtools-extension": "^2.13.5",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "http-proxy-middleware": "^0.19.0",
    "immutable": "^3.8.2",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-saga": "^0.16.2",
    "reselect": "^4.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint:js"
    }
  }
}
